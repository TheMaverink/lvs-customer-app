{
  "expo": {
    "name": "lvs-customer-frontend",
    "slug": "lvs-customer-frontend",
    "privacy":"public",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.julio.lvs"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
