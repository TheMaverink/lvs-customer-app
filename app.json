{
  "expo": {
    "name": "LVS",
    "slug": "lvs",
    "description":"Car wash app",
    "privacy": "public",
    "version": "15.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "android": {
      "package": "com.julio.lvs",
      "versionCode": 1
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    },
    "orientation": "portrait",
    "icon": "./assets/logos/icon.png",
    "backgroundColor": "#000000",
    "splash": {
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.julio.lvs"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}